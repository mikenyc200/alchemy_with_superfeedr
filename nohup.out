  [1m[35mSQL (0.2ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.fastcoexist.com/3046998', 'Sick Of Your Drab Office? Try This Coworking Space In The Canopy Of A Tree', 'http://feedproxy.google.com/~r/fastcompany/headlines/~3/tiyaPgCwdfE/sick-of-your-drab-office-try-this-coworking-space-in-the-canopy-of-a-tree', 'http://h.fastcompany.net/multisite_files/fastcompany/imagecache/620x350/poster/2015/06/3046998-poster-p-1-this-coworking-office-is-in-a-park-and-you-pay-your-membership-fee-to-a-tree.jpg', 2, '2015-06-04 16:33:46', '2015-06-04 16:33:46')
