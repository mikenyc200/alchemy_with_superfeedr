  [1m[35mSQL (0.2ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.fastcompany.com/3047066', 'Twitter Just Made It Easier To Find Strangers\' Videos On Periscope', 'http://feedproxy.google.com/~r/fastcompany/headlines/~3/rGx9yVyFJyI/twitter-just-made-it-easier-to-strangers-videos-on-periscope', 'http://d.fastcompany.net/multisite_files/fastcompany/imagecache/620x350/poster/2015/06/3047066-poster-p-1-twitters-periscope-app-adds-an-interactive-world-map-to-help-you-find-videos.jpg', 2, '2015-06-04 16:33:39', '2015-06-04 16:33:39')
