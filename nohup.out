  [1m[35mSQL (0.1ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/us/breach-in-a-federal-computer-system-exposes-personnel-data.html', 'Breach in a Federal Computer System Exposes Personnel Data', 'http://rss.nytimes.com/c/34625/f/667200/s/46f2b4ac/sc/28/l/0L0Snytimes0N0C20A150C0A60C0A50Cus0Cbreach0Ein0Ea0Efederal0Ecomputer0Esystem0Eexposes0Epersonnel0Edata0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 5, '2015-06-05 14:04:55', '2015-06-05 14:04:55')
