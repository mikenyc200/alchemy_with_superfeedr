  [1m[35mSQL (0.2ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/04/business/media/josh-duggar-molested-four-of-his-sisters-his-parents-tell-fox-news.html', 'Josh Duggar Molested Four of His Sisters, His Parents Tell Fox News', 'http://rss.nytimes.com/c/34625/f/667200/s/46ed3b84/sc/19/l/0L0Snytimes0N0C20A150C0A60C0A40Cbusiness0Cmedia0Cjosh0Eduggar0Emolested0Efour0Eof0Ehis0Esisters0Ehis0Eparents0Etell0Efox0Enews0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 'http://graphics8.nytimes.com/images/2015/06/04/business/media/04duggars1/04duggars1-moth.jpg', 4, '2015-06-04 16:58:37', '2015-06-04 16:58:37')
