  [1m[35mSQL (0.2ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/world/europe/fifa-jack-warner-sepp-blatter-corruption.html', 'Ex-FIFA Official Jack Warner Threatens to Spill ‘Avalanche’ of Secrets', 'http://rss.nytimes.com/c/34625/f/667200/s/46efd0fe/sc/3/l/0L0Snytimes0N0C20A150C0A60C0A50Cworld0Ceurope0Cfifa0Ejack0Ewarner0Esepp0Eblatter0Ecorruption0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 'http://graphics8.nytimes.com/images/2015/06/05/world/05Fifa-web/05Fifa-web-moth.jpg', 4, '2015-06-04 16:58:30', '2015-06-04 16:58:30')
