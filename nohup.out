  [1m[35mSQL (0.1ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/world/africa/burundi-postpones-elections-amid-unrest.html', 'Burundi Postpones Elections Amid Unrest', 'http://rss.nytimes.com/c/34625/f/667200/s/46f30296/sc/40/l/0L0Snytimes0N0C20A150C0A60C0A50Cworld0Cafrica0Cburundi0Epostpones0Eelections0Eamid0Eunrest0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 5, '2015-06-05 14:04:56', '2015-06-05 14:04:56')
