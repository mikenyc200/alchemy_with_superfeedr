  [1m[35mSQL (0.1ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/world/europe/hardening-of-positions-undercuts-ukraine-poroshenko-peace-accord.html', 'Hardening of Positions Undercuts Ukraine Peace Accord', 'http://rss.nytimes.com/c/34625/f/667200/s/46f43e32/sc/11/l/0L0Snytimes0N0C20A150C0A60C0A50Cworld0Ceurope0Chardening0Eof0Epositions0Eundercuts0Eukraine0Eporoshenko0Epeace0Eaccord0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 'http://graphics8.nytimes.com/images/2015/06/05/world/UKRAINE/UKRAINE-moth.jpg', 5, '2015-06-05 14:04:50', '2015-06-05 14:04:50')
