  [1m[35mSQL (0.2ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/world/asia/yangtze-river-capsized-ship-oriental-star-rescue-efforts.html', 'Relatives of Yangtze Ship Passengers Seek Answers Amid Search for Survivors', 'http://rss.nytimes.com/c/34625/f/667200/s/46efd0ff/sc/19/l/0L0Snytimes0N0C20A150C0A60C0A50Cworld0Casia0Cyangtze0Eriver0Ecapsized0Eship0Eoriental0Estar0Erescue0Eefforts0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 'http://graphics8.nytimes.com/images/2015/06/05/world/05Yangtze1-web/05Yangtze1-web-moth.jpg', 4, '2015-06-04 16:58:27', '2015-06-04 16:58:27')
