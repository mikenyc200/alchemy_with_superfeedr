  [1m[35mSQL (0.1ms)[0m  INSERT INTO `entries` (`atom_id`, `title`, `url`, `image`, `feed_id`, `created_at`, `updated_at`) VALUES ('http://www.nytimes.com/2015/06/05/sports/soccer/one-day-gianluigi-buffon-wants-to-call-the-shots-not-stop-them.html', 'One Day, Italy’s Gianluigi Buffon Wants to Call the Shots, Not Stop Them', 'http://rss.nytimes.com/c/34625/f/667200/s/46f48097/sc/13/l/0L0Snytimes0N0C20A150C0A60C0A50Csports0Csoccer0Cone0Eday0Egianluigi0Ebuffon0Ewants0Eto0Ecall0Ethe0Eshots0Enot0Estop0Ethem0Bhtml0Dpartner0Frss0Gemc0Frss/story01.htm', 'http://graphics8.nytimes.com/images/2015/06/05/sports/BUFFONsilo/BUFFONsilo-moth.jpg', 5, '2015-06-05 14:04:48', '2015-06-05 14:04:48')
